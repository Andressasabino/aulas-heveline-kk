#include <stdio.h>
int main()
{
 int n1,n2,n3,media;
  printf("digite a primeira nota:\n");
  scanf("%d",&n1);
  
  printf("digitea segunda nota: \n");
  scanf("%d",&n2);
  
  printf("digite a terceira nota:\n");
  scanf("%d",&n3);
  
  media = (n1 + n2 + n3)/3;

  
  printf("\n A sua media foi: %d", media);
  
  return 0;
}
